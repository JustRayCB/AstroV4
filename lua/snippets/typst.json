{
  "Section": {
    "prefix": ["h"],
    "body": ["= ${1:Heading Name} <h:${2:ref}>"],
    "description": "Section"
  },
  "Subsection": {
    "prefix": ["sh"],
    "body": ["== ${1:Subheading Name} <sh:${2:ref}>"],
    "description": "Subsection"
  },
  "Subsubsection": {
    "prefix": ["ssh"],
    "body": ["=== ${1:SubsubheadingName} <ssh:${2:ref}>"],
    "description": "Subsubsection"
  },
  "Subsubsubsection": {
    "prefix": ["sssh"],
    "body": ["==== ${1:Subsubsubheading Name} <sssh:${2:ref}>"],
    "description": "Subsubsubsection"
  },
  "Ordered List": {
    "prefix": ["ol"],
    "body": ["+ ${1:Item 1}", "+ ${2:Item 2}", "+ ${3:Item 3}"],
    "description": "Ordered List"
  },
  "Unordered List": {
    "prefix": ["ul"],
    "body": ["- ${1:Item 1}", "- ${2:Item 2}", "- ${3:Item 3}"],
    "description": "Unordered List"
  },
  "Image": {
    "prefix": ["img"],
    "body": ["#image(\"${1:Image Name}\", width: ${2:70}%)"],
    "description": "Insert an Image"
  },
  "Figure": {
    "prefix": ["fig"],
    "body": [
      "#figure(",
      "\timage(\"${1:Image Name}\", width: ${2:70}%),",
      "\tcaption: [",
      "\t\t${3:Caption}",
      "\t],",
      ")<fig:${4:figureName}>"
    ],
    "description": "Insert a Figure"
  },
  "InlineMath": {
    "prefix": ["im", "ie"],
    "body": ["$$1$"],
    "description": "Insert Inline math equation"
  },
  "Math Equation": {
    "prefix": ["eq", "math"],
    "body": ["$\n $1 \n$"],
    "description": "Insert math equation"
  },
  "Sum": {
    "prefix": ["sum"],
    "body": ["sum_(${1:i=0})^(${2:n}) $3 "],
    "description": "Sum function, sigma"
  },
  "Fraction": {
    "prefix": ["frac"],
    "body": ["(${1:num}) / (${2:denom}) "],
    "description": "Fraction"
  },
  "Italic": {
    "prefix": ["i"],
    "body": ["_$1_ "]
  },
  "Bold": {
    "prefix": ["b"],
    "body": ["*$1* "]
  },
  "Code": {
    "prefix": ["code"],
    "body": ["```$1", "$2", "```"],
    "description": "Code block typst"
  },
  "Underline": {
    "prefix": ["u"],
    "body": ["#underline[$1]"]
  },
  "Color": {
    "prefix": ["clr"],
    "body": ["#text(${1:green})[$2]"]
  },
  "Quote": {
    "prefix": ["qte"],
    "body": [
      "#quote(block: ${1:true}, quotes: ${2:true}, attribution: \"${3:Author}\")[$4]"
    ]
  },
  "Definition": {
    "prefix": ["def"],
    "body": [
      "#definition[${1:Title}][",
      "\t${2:Definition}",
      "]<def:${3:ref}>"
    ],
    "description": "Definition box"
  },
  "Theorem": {
    "prefix": ["thm"],
    "body": ["#theorem[${1:Title}][", "\t${2:Theorem}", "]<thm:${3:ref}>"],
    "description": "Theorem box"
  },
  "Proof": {
    "prefix": ["proof"],
    "body": ["#proof[${1:Title}][", "\t${2:Proof}", "]<prf:${3:ref}>"],
    "description": "Proof box"
  },
  "Example": {
    "prefix": ["ex"],
    "body": ["#example[${1:Title}][", "\t${2:Example}", "]<ex:${3:ref}>"],
    "description": "Example box"
  },
  "Proposition": {
    "prefix": ["prop"],
    "body": [
      "#proposition[${1:Title}][",
      "\t${2:Proposition}",
      "]<prop:${3:ref}>"
    ],
    "description": "Proposition box"
  },
  "Corollary": {
    "prefix": ["cor"],
    "body": ["#corollary[${1:Title}][", "\t${2:Corollary}", "]<cor:${3:ref}>"],
    "description": "Corollary box"
  },
  "Lemma": {
    "prefix": ["lem"],
    "body": ["#lemma[${1:Title}][", "\t${2:Lemma}", "]<lem:${3:ref}>"],
    "description": "Lemma box"
  },
  "Remark": {
    "prefix": ["rem"],
    "body": ["#remark[${1:Title}][", "\t${2:Remark}", "]<rem:${3:ref}>"],
    "description": "Remark box"
  },
  "Notation": {
    "prefix": ["not"],
    "body": ["#notation[${1:Title}][", "\t${2:Notation}", "]<not:${3:ref}>"],
    "description": "Notation box"
  },
  "Warning": {
    "prefix": ["warn"],
    "body": ["#warning[${1:Title}][", "\t${2:Warning}", "]<warn:${3:ref}>"],
    "description": "Warning box"
  },
  "Problem": {
    "prefix": ["prob"],
    "body": ["#problem[${1:Title}][", "\t${2:Problem}", "]<prob:${3:ref}>"],
    "description": "Problem box"
  },
  "Indent": {
    "prefix": ["ind"],
    "body": ["#indent()"],
    "description": "Indent text"
  },
  "NoIndent": {
    "prefix": ["nind"],
    "body": ["#noindent()"],
    "description": "No indent text"
  },
  "Highlight": {
    "prefix": ["hl"],
    "body": ["#highlight(fill: ${1:aqua})[${2:Text}]"],
    "description": "Highlight text"
  },
  "Term": {
    "prefix": ["trm"],
    "body": ["/ ${1:Term}: ${2:Definition}."],
    "description": "Term definition"
  },
  "Un-numbered Section": {
    "prefix": ["un"],
    "body": ["#unnumbered[${1:Title}]"],
    "description": "Un-numbered Section"
  }
}
